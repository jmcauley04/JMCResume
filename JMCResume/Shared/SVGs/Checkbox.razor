<svg x="@X" y="@Y" viewBox="0 0 32 32">
    <rect width="10" height="10" x=16 y=16 style="fill:none;stroke-width:1;stroke:#58346b" />
    @if (Checked)
    {
        <polygon points="18,18 21.5,21 31,11 21,25" style="fill:rgb(132,192,99)" />
    }
</svg>

@code {
    [Parameter]
    public bool Checked { get; set; }

    [Parameter]
    public double X { get; set; } = 0;

    [Parameter]
    public double Y { get; set; } = 0;
}
